<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
	
	<GenericTile
		header="{i18n>Home.generic.title1}"
		class="sapUiTinyMarginBegin sapUiTinyMarginTop">
		<TileContent>
			<ImageContent src="sap-icon://multiple-line-chart"/>
		</TileContent>
	</GenericTile>
	
	<GenericTile
		header="{i18n>Home.generic.title2}"
		class="sapUiTinyMarginBegin sapUiTinyMarginTop">
		<TileContent>
			<ImageContent src="sap-icon://chart-table-view"/>
		</TileContent>
	</GenericTile>
	
	<GenericTile
		header="{i18n>Home.generic.title3}"
		class="sapUiTinyMarginBegin sapUiTinyMarginTop">
		<TileContent>
			<ImageContent src="sap-icon://legend"/>
		</TileContent>
	</GenericTile>
	
	<GenericTile
		header="{i18n>Home.generic.title4}"
		class="sapUiTinyMarginBegin sapUiTinyMarginTop">
		<TileContent>
			<ImageContent src="sap-icon://functional-location"/>
		</TileContent>
	</GenericTile>

</mvc:View>